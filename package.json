{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.22.0",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "eslint .",
    "start": "next start",
    "amp": "amp",
    "amp:build": "pnpm amp build -c amp.config.ts -o ./infra/amp/datasets/evvm_explorer.json",
    "amp:build:base": "ampctl manifest generate --network sepolia --kind evm-rpc --out ./infra/amp/datasets/sepolia_base.json",
    "amp:register:base": "ampctl dataset register sepolia/base -t 0.0.1 ./infra/amp/datasets/sepolia_base.json",
    "amp:register": "ampctl dataset register evvm/evvm_explorer ./infra/amp/datasets/evvm_explorer.json",
    "amp:deploy:base": "ampctl dataset deploy sepolia/base@dev",
    "amp:deploy": "ampctl dataset deploy evvm/evvm_explorer@dev",
    "amp:dev": "pnpm amp dev",
    "amp:server": "ampd --config infra/amp/config.toml dev",
    "amp:studio": "pnpm amp studio --open",
    "amp:query": "bash scripts/amp-query.sh",
    "amp:setup": "pnpm amp:build:base && pnpm amp:register:base && pnpm amp:deploy:base && pnpm amp:build && pnpm amp:register && pnpm amp:deploy",
    "forge:install-deps": "bash scripts/install-dependencies.sh",
    "forge:build": "forge build",
    "forge:deploy": "forge script scripts/DeployEvvmWithEvents.s.sol:DeployEvvmWithEvents --rpc-url $SEPOLIA_RPC_URL --broadcast -vvvv",
    "forge:deploy:base": "forge script scripts/DeployEvvmWithEvents.s.sol:DeployEvvmWithEvents --rpc-url $BASE_SEPOLIA_RPC_URL --broadcast -vvvv",
    "forge:deploy:base:verify": "forge script scripts/DeployEvvmWithEvents.s.sol:DeployEvvmWithEvents --rpc-url $BASE_SEPOLIA_RPC_URL --broadcast --verify --etherscan-api-key $BASESCAN_API_KEY -vvvv",
    "forge:deploy:base-mainnet": "bash -c 'source .env 2>/dev/null || true; forge script scripts/DeployEvvmWithEvents.s.sol:DeployEvvmWithEvents --rpc-url ${BASE_RPC_URL:-https://mainnet.base.org} --broadcast -vvvv'",
    "forge:deploy:base-mainnet:verify": "bash -c 'source .env 2>/dev/null || true; forge script scripts/DeployEvvmWithEvents.s.sol:DeployEvvmWithEvents --rpc-url ${BASE_RPC_URL:-https://mainnet.base.org} --broadcast --verify --etherscan-api-key ${BASESCAN_API_KEY} -vvvv'",
    "forge:deploy:verify": "forge script scripts/DeployEvvmWithEvents.s.sol:DeployEvvmWithEvents --rpc-url $SEPOLIA_RPC_URL --broadcast --verify --etherscan-api-key $ETHERSCAN_API_KEY -vvvv",
    "forge:test-transactions": "bash scripts/generate-transactions.sh",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-aspect-ratio": "1.1.1",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-collapsible": "1.1.2",
    "@radix-ui/react-context-menu": "2.2.4",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-hover-card": "1.1.4",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-menubar": "1.1.4",
    "@radix-ui/react-navigation-menu": "latest",
    "@radix-ui/react-popover": "1.1.4",
    "@radix-ui/react-progress": "1.1.1",
    "@radix-ui/react-radio-group": "1.2.2",
    "@radix-ui/react-scroll-area": "1.2.2",
    "@radix-ui/react-select": "2.1.4",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slider": "1.2.2",
    "@radix-ui/react-slot": "latest",
    "@radix-ui/react-switch": "1.1.2",
    "@radix-ui/react-tabs": "latest",
    "@radix-ui/react-toast": "1.2.4",
    "@radix-ui/react-toggle": "1.1.1",
    "@radix-ui/react-toggle-group": "1.1.1",
    "@radix-ui/react-tooltip": "1.1.6",
    "@vercel/analytics": "latest",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "date-fns": "4.1.0",
    "embla-carousel-react": "8.5.1",
    "input-otp": "1.4.1",
    "lucide-react": "^0.454.0",
    "next": "16.0.3",
    "next-themes": "latest",
    "react": "19.2.0",
    "react-day-picker": "9.8.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.60.0",
    "react-resizable-panels": "^2.1.7",
    "recharts": "2.15.4",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@edgeandnode/amp": "^0.0.51",
    "@tailwindcss/postcss": "^4.1.9",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "postcss": "^8.5",
    "tailwindcss": "^4.1.9",
    "tw-animate-css": "1.3.3",
    "typescript": "^5"
  }
}
